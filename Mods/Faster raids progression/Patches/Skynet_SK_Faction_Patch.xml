<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Skynet</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<!-- Skynet -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[defName="SkynetHumanlike"]/earliestRaidDays</xpath>
					<value>
						<earliestRaidDays>140</earliestRaidDays>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[defName="SkynetHumanlike"]/raidCommonalityFromPointsCurve</xpath>
					<value>
						<raidCommonalityFromPointsCurve>
							<points>
								<li>(0, 0)</li>
								<li>(8000, 0)</li>
								<li>(9000, 0.7)</li>
								<li>(13000, 1.1)</li>
								<li>(17000, 1.15)</li>
							</points>
						</raidCommonalityFromPointsCurve>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[defName="SkynetHumanlike"]/maxPawnCostPerTotalPointsCurve</xpath>
					<value>
						<maxPawnCostPerTotalPointsCurve>
							<points>
								<li>(0,600)</li>
								<li>(11500,650)</li>
								<li>(14000,1000)</li>
								<li>(17000,1750)</li>
								<li>(100000,10000)</li>
							</points>
						</maxPawnCostPerTotalPointsCurve>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>